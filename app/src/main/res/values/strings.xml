    <string name="settings_timing_title">Timing</string>
    <string name="settings_timing_clear_timeout_title">Clear timeout</string>
    <string name="settings_timing_clear_timeout">The time after which a wrong pattern is cleared. Default: 2000ms</string>
    <string name="settings_timing_appear_animation_duration_title">Appear animation duration</string>
    <string name="settings_timing_appear_animation_duration">The duration in takes for the dots to move up from the bottom and fade in. Default: 220ms</string>
    <string name="settings_timing_appear_animation_start_translation_title">Appear animation start translation</string>
    <string name="settings_timing_appear_animation_start_translation">The multiplier to shift the dots down before the animation starts. Default: 1.5x</string>
    <string name="settings_timing_appear_animation_delay_scale_title">Appear animation delay scale</string>
    <string name="settings_timing_appear_animation_delay_scale">The delay it takes per dot to appear after each other. Default: 2.0x</string>
    <string name="settings_timing_disappear_animation_duration_title">Disappear animation duration</string>
    <string name="settings_timing_disappear_animation_duration">The duration in takes for the dots to move up from the pattern view and fade out. Default: 125ms</string>
    <string name="settings_timing_disappear_animation_start_translation_title">Disappear animation start translation</string>
    <string name="settings_timing_disappear_animation_start_translation">The multiplier to move the dots up during the animation. Default: 1.2x</string>
    <string name="settings_timing_disappear_animation_delay_scale_title">Disappear animation delay scale</string>
    <string name="settings_timing_disappear_animation_delay_scale">The delay it takes per row to disappear after each other. Default: 0.8x</string>

